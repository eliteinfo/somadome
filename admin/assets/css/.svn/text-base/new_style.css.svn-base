body {margin: 0px; padding:0px; font-family:'GothamHTFBook'; font-size:14px; line-height:23px; font-weight:normal; color:#41372e;}

h1 , h2 , h3 , form, input{ margin:0; padding:0;}

h1 { font-family:'MuseoRegular'; font-size:30px; line-height:13px; color:#ad0006; padding:26px 0 25px 35px }
h2 { font-family:'Museo300'; font-size:20px; line-height:13px; color:#ad0006; font-weight:normal;  }
h3{font-size:14px; color:#242424; font-weight:bold; margin:0px; padding-bottom:6px; font-family:Helvetica;}

a{ color:#0a3f83; text-decoration:none;}
img{border:0}
a:hover{ color:#212020; text-decoration:none;}


.wrapper { width:1008px; margin:0 auto; padding:0;    }

.clear { clear:both;}

