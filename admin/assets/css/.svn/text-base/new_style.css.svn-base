/*top*/

/*menu*/
ul.menu {padding:0; margin:5px 5px 0 0; float:right; width:auto; list-style-type:none;}
ul.menu li{ float:left; position:relative; z-index:9999; padding:2px 5px;}
ul.menu li a:link, ul.menu li a:visited{text-decoration:none; color:#fff; padding:0px 15px; float:left; cursor:pointer;font-family: 'GothamHTFMedium'; font-weight:normal; font-size:13px; line-height:33px; text-transform:uppercase;}
ul.menu li a:hover { background-color:#d9050e; }
ul.menu li a.menuhover {color:#; background:#d9050e; bottom center no-repeat;}
ul.menu li.current a {color:#fff; background:#d9050e; bottom center no-repeat;}

ul.menu ul {position:absolute; top:41px; left:0;  display:none; list-style:none; margin:0; padding:0}
ul.menu ul li{position:relative; margin:0; font-size:12px; font-weight:normal; padding:0; border-bottom:#000 1px solid; }
ul.menu ul li a:link, ul.menu ul li a:visited {display:block; margin:0px; padding:5px 5px 5px 10px;color:#fff; text-decoration:none; font-size:12px;background:#877463; height:auto; width:145px; font-weight:normal; line-height:normal; border:none; text-transform:none;  }
ul.menu ul li a:hover { background:#d9050e;color:#fff; border:none;}
ul.menu ul ul {left:160px; top:0px;}


/*top css start*/
.top { float:left; background:url(../../images/top_bg.jpg) repeat-x; width:100%; height:46px; position:relative; z-index:9999;}
.logo { float:left; padding-left:38px; width:176px;}
/*top css end*/

/*banner css start*/
.banner {  float:left; width:100%; min-height:610px; overflow:hidden; position: relative; background-color:#b8977e; }
.banner_img { z-index:1; position:absolute; top:0; left:0;width:100%; text-align:center; }
.banner_info { float:left; width:100%; position:relative; z-index:99;}
.round { position:absolute; top:48px; left:0; width:100%; }
.wrap_box { width:auto; float:right; }
.login_btn { position:absolute; background:url(../../images/login.png) no-repeat; right:35px; top:30px; width:210px; height:27px;   }
ul.login_ul { margin:0; padding:0; }
ul.login_ul li { float:left; list-style-type:none; border-left:1px solid #8ee640; padding:1px 0px 0 11px; 
background:url(../../images/green_arrow.png) 13px 11px no-repeat ;  }
ul.login_ul li a:hover { color:#e8ddd4; }
ul.login_ul li:first-child { border:none; }
ul.login_ul li a { font-family:'ralewaymedium'; font-size:11px; color:#fff; text-transform:uppercase; padding:4px 11px 0 5px; }

.form {position:absolute; margin-left:65px;}
.form_bg { background:url(../../images/banner_form_bg.png) center no-repeat; width:476px; margin:0 auto;    }
.form h2 { text-align:center; font-family:'Museo300'; font-size:26px; line-height:25px; color:#4d472f; margin-top: 71px; }
.form h2 span { font-family:'Museo-700'; font-weight:normal;  }
.title { margin-top:58px; }

.title .left { width:auto; float:left; margin:0 3px 0 33px;   }
.left span { padding-right:10px; text-transform:uppercase;  }
.left img { margin-bottom:-15px; }  

.title .right { width:auto; float:right;   }
.right span { padding-right:10px; text-transform:uppercase;  }
.right img { margin-bottom:-15px; }  

.field { width:350px; margin:0 auto; padding-bottom:150px; }
.field .select { float:left; width:150px; background:#fff url(../../images/right_arrow.jpg) right no-repeat; border:1px solid #CCC; margin:20px 0 0 14px; overflow:hidden; padding:12px 0;   }

.field .select select { width:172px; border:none; background:none; float:left; padding-left:15px; font-family:'ralewaymedium'; font-size:15px; line-height:47px; color:#393939;  }
.field .select_country select { width:330px; border:none; background:none; float:left; padding-left:15px; font-family:'ralewaymedium'; font-size:15px; line-height:47px; color:#393939;  }

.field .select_country  { float:left; width:312px; background:#fff url(../../images/right_arrow.jpg) right no-repeat; border:1px solid #CCC; margin:20px 0 0 14px; overflow:hidden; padding:12px 0;   }

.submit_btn { background:url(../../images/search.jpg) center no-repeat; width:188px; height:41px; cursor:pointer; border:none; float:left; margin:20px 0 0 85px;   }

/*banner css end*/


/*mid part css start*/
.mid_part { float:left; width:100%; background-color:#ebfafa;}
.content { float: left; width:100%; padding-bottom:90px;  }
.content h1 { font-family:'MuseoRegular'; font-size:45px; line-height:50px; color:#dd0712; text-align:center; padding:92px 0 00px 0;     }
.content p { font-family:'ralewaymedium'; font-size:17px; color:#242424; line-height:30px; text-align:center;   }
.round_box { width:100%; float:left;  margin-left: 90px }

.round_box .box_1 { float:left; width:253px; padding:38px 45px 0 0 }
.box_1 h1 { font-family:'MuseoRegular'; font-size:25px; line-height:24px; color:#171717; margin:0;padding-top:33px;  }

.round_box .box_2 { float:left; width:253px; padding:38px 45px 0 0 }
.box_2 h1 { font-family:'MuseoRegular'; font-size:25px; line-height:24px; color:#171717; margin:0;padding-top:33px; }

.round_box .box_3 { float:left; width:253px; padding:38px 45px 0 0 }
.box_3 h1 { font-family:'MuseoRegular'; font-size:25px; line-height:24px; color:#171717; margin:0; padding-top:33px;  }

/*mid part css end*/

/*bottom content css start*/
.btm_bg { float:left;  width:100%; background-color:#f9f5d4;  }
.btm_content { float:left; position:relative; width:100%;  }
.coma_left { background:url(../../images/coma_left.png) center no-repeat; width:90px; height:73px; left:46px; top:77px; position:absolute; }
.btm_text { float:left; width:100%; padding:62px 0 21px 0  }
.btm_text p { font-family:'BookmanITCStdLightItalic'; font-size:18px; text-align:center; line-height:32px; color:#535249; }
.btm_text span { font-family:Arial; font-size:12px; line-height:30px; color:#dd0712; font-weight:bold;   }
.coma_right { background:url(../../images/coma_right.png) center no-repeat; width:90px; height:73px; right:60px; top:77px; position:absolute; }
/*bottom content css end*/


/*footer css start*/
.footer { float:left; width:100%; background:url(../../images/footer_bg.jpg) repeat-x; }
.footer_content { float:left; width:100%; padding:20px 0 20px 40px; }
.footer_content a { color:#fff; }
.footer_content a:hover { text-decoration:underline; }


.member { float:left; width: 230px; padding-right: 15px;  }
.member span { font-family:'open_sansbold'; font-size:12px; line-height:23px; color:#ffcd00; text-transform:uppercase;    }
ul.mem_ul { margin:0; padding:0 0 0 0; }
ul.mem_ul li { list-style-type:none; font-family:'open_sansregular'; font-size:12px; color:#fff;   }


.info { float:left; width: 230px; padding-right:100px;  }
.info span { font-family:'open_sansbold'; font-size:12px; line-height:23px; color:#ffcd00; text-transform:uppercase;    }
ul.info_ul { margin:0; padding:0 0 0 0; }
ul.info_ul li { list-style-type:none; font-family:'open_sansregular'; font-size:12px;color:#fff; width:115px; float: left;  }


.widgets { float:left; padding-right: 49px; }
.widgets span { font-family:'open_sansbold'; font-size:12px; line-height:23px; color:#ffcd00; text-transform:uppercase;    }
ul.wid_ul { margin:0; padding:0 0 0 0; }
ul.wid_ul li { list-style-type:none; font-family:'open_sansregular'; font-size:12px; color:#fff;  }

.follow { float:left; padding-right: 20px; }
.follow span { font-family:'open_sansbold'; font-size:12px; line-height:23px; color:#ffcd00; text-transform:uppercase;    }
ul.fol_ul { margin:0; padding:0 0 0 0; }
ul.fol_ul li { list-style-type:none; font-family:'open_sansregular'; font-size:12px; color:#fff; float:left; padding-right:10px;    }
.follow p { font-family:'open_sansregular'; font-size:12px; height:23px; color:#fff; margin:90px 0 0 20px; }

/*footer css end*/


/*about page css start*/
.title_bg { float:left; width:100%; background:url(../../images/about_head_bg.jpg) center repeat; height:73px;  }
.page_title { float:left; width:auto;  }
.login_btn2 { float:right; background:url(../../images/login.png) no-repeat; width:198px; height:27px; margin:21px 35px;    }
ul.login_ul2 { margin:0; padding:0; }
ul.login_ul2 li { float:left; list-style-type:none; border-left:1px solid #8ee640; padding:1px 6px 0 20px; 
background:url(../../images/green_arrow.png) 13px 11px no-repeat ;  }
ul.login_ul2 li a:hover { color:#e8ddd4; }
ul.login_ul2 li:first-child { border:none; }
ul.login_ul2 li a { font-family:'ralewaymedium'; font-size:11px; color:#fff; text-transform:uppercase; padding:4px 11px 0 5px; }

/*page_content cs start*/
.page_content { float:left; width:926px; padding:25px 42px;    }
/*page_content cs end*/
/*about page css end*/

/*search css start*/
.search_ul { margin:0 43px; padding:20px 0; float:left;  }
.search_ul li { float:left; list-style-type:none; }
.search_ul li a { font-family:'GothamHTFMedium'; font-size:13px; line-height:13px; color:#41372e; border-left:1px solid #000; padding:0px 5px 0 5px;  }
.search_ul li:first-child a { border-left:none;  }
.welcome { float:right; margin:30px 30px 15px 0;  }
.welcome span { float:left; display:block; clear:both; text-align:right; width: 100%; font-family:'GothamHTFBook'; font-size:13px; line-height:13px; color:#41372e;    }
.welcome span a { color:#ad0006; }
/*search css end*/

/*search content start*/
.search_content { float:left; width:100%; }
.refine_box { float:left; width:213px; background-color:#ebfafa; min-height:1000px; padding:27px 5px; border:1px solid #c9eaea; }
.refine_box h2 { font-family:'Museo300'; font-size:20px; line-height:13px; color:#ad0006; font-weight:normal; padding-left:12px;   }
ul.refine { margin:10px 0 0 0; padding:0;  }
ul.refine li span { font-family:'GothamHTFMedium'; font-size:13px; line-height:19px; color:#41372e; }
ul.refine li { list-style-type:none; padding:15px 0 15px 12px; border-bottom:1px solid #c9eaea; float:left; width:201px;  background:url(../../../images/plus.jpg) right 24px no-repeat;     }
ul.refine li.active { background:url(../../images/plus.jpg) right 24px no-repeat;   }
ul.refine ul { margin:0; padding:0; }
ul.refine ul li { padding:0; border-bottom:none; background-image:none;   }
ul.refine ul li a { font-family:'GothamHTFBook'; font-size:12px;  line-height:19px; color:#4a9e00; float:left;   }

/*search content end*/

/*Right Portfolio css start*/
.port_box1 { float:right; width:715px; padding:20px; margin-top:20px;     }
.port_box2 { float:right; min-height:1000px; width:715px; padding:20px;    }
.port_box1 .port_detail { border-bottom:none; }
.port_detail { float:left; width:100%; padding:10px 0; border-bottom: 1px solid #DADBDB;margin-bottom: 15px;  }
.port_content { width:100%;  }
.top_name { float:left; width:100%; padding-bottom: 25px;}
.prof_name { float:left; }
.prof_name span { font-family:'GothamHTFBook'; font-size:18px;  line-height:13px; color:#4a9e00;   }
.view { float:right;  }
.view span  { background:url(../../images/prof_arrow.jpg) left 8px no-repeat; width:3px; height:5px; padding:2px 15px 3px 10px;  }
.view span a { font-family:'GothamHTFBook'; font-size:13px;  line-height:27px; color:#4a9e00;   }

.prof_img { float:left; }
.pic_bg { border:1px solid #9d9a7f;margin: 0 20px 10px 0;  }
.forward { float:left; width:100%;   }
.forward span { float:left ;font-family:'GothamHTFBook'; font-size:12px;  line-height:27px; color:#242424; margin-right:22px;}
.forward .picture { float:right; }
.forward span a { font-family:'GothamHTFBook'; font-size:13px;  line-height:27px; color:#4a9e00; background:url(../../images/prof_arrow.jpg) left 8px no-repeat; padding:2px 15px 3px 10px;   }

.pro_info { float:left; margin: 0 20px 0 0;}
.login_box { float:left; }
.login_box .wel_text { font-family:Georgia; font-size:13px; font-weight:normal; font-style:italic; color:#352c24; float:left; margin-top:40px;   }
.login_box .wel_text a { font-family:Georgia; font-size:13px; font-weight:normal; font-style:italic; color:#352c24; color:#4a9e00;}

.prof_detail { float:right; width:296px; margin: 0 20px 0 0; }
.selected { background-color:#f2f1e1; }
.prof_detail  h5 { float:left; font-family:' VolkhovRegular'; font-size:14px; line-height:22px; color:#877463; font-weight:bold; margin:0; padding:0;    }
.prof_detail table { font-size:12px; line-height:normal;  }
.prof_detail table span { float:left; font-family:'GothamHTFMedium'; font-size:12px; line-height:27px; color:#352c24; margin:0; padding:0;  }

.full_prof { float:left; background:url(../../images/prof_arrow.jpg) right 25px no-repeat ;padding:17px 18px 0 2px; font-family:'VolkhovRegular'; font-size:12px; line-height:22px; text-transform:uppercase; }
.full_prof span a { color:#4a9e00; }

.nav_bar { float:left; width:100%; margin:1px 0 0 -20px;  }
.nav_for { float:left; margin:10px 0 0 3px; }
.nav_for input[type=checkbox] { float:left; margin:8px 7px 0 0  }
.nav_for label { float:left; font-family:'GothamHTFMedium'; font-size:13px; line-height:27px; color:#000; }

.navigation { float:right;  }
ul.nav { margin:0; padding:0; float:right; margin:20px 0;   }
ul.nav li { float:left; list-style-type:none; border:1px solid #CCC; border-radius:4px; margin-left:3px;  }
ul.nav li.first { background:url(../../images/box_arrow.jpg) center no-repeat;   }
ul.nav li.first a { text-indent:-9999px; padding:2px 12px; }
ul.nav li.last a { text-indent:-9999px; padding:2px 12px; }

ul.nav li.first a:hover { background:#877463 url(../../images/white_arrow1.png) center no-repeat; }
ul.nav li.last a:hover { background:#877463 url(../../images/white_arrow2.png) center no-repeat; }

ul.nav li.last { background:url(../../images/box_arrow_right.jpg) center no-repeat;   }
ul.nav li a {  display:block; cursor:pointer; color:#717171; float:left; padding:2px 9px; font-family:Helvetica; font-size:12px; }
ul.nav li a:hover { background-color:#877463; color:#fff; }
/*Right Portfolio css end*/



